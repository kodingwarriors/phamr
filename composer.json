{
  "name": "kodingwarriors/phamr",
  "type": "library",
  "description": "This is a sample application for the Phalcon PHP Framework with HMVC structure Base on Vokuro Skeleton",
  "keywords": [
    "phalcon",
    "framework",
    "sample app",
    "modular",
    "hmvc"
  ],
  "homepage": "https://kodingwarriors.github.io/",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dwi Agus Purwanto",
      "email": "dwi.agus.pur@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-openssl": "*",
    "ext-phalcon": "~5.0.0",
    "vlucas/phpdotenv": "^3.6"
  },
  "suggest": {
    "ext-apc": "Needed to support caching ACL"
  },
  "autoload": {
    "psr-4": {
      "Phamr\\": "src/"
    },
    "files": [
      "src/Helpers.php"
    ]
  }
}